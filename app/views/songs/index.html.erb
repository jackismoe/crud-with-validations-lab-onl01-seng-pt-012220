<h1>Songs</h1>

<div><%= link_to 'Create Song', new_song_path %></div>
<br />

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Year</th>
      <th>Genre</th>
      <th>Artist Name</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @songs.each do |x| %>
      <tr>
        <td><%= x.title %></td>
        <td><%= x.release_year %></td>
        <td><%= x.genre %></td>
        <td><%= x.artist_name %></td>
        <td>
          <%= link_to 'View', x %> |
          <%= link_to 'Edit', edit_song_path(x)%> |
          <%= link_to 'Delete', x, method: :delete, data: {confirm: 'For Real?'} %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
